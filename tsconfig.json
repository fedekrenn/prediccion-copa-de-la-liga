{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@utils/*": ["./src/utils/*"],
      "@services/*": ["./src/services/*"],
      "@assets/*": ["./src/assets/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@styles/*": ["./src/components/styles/*"],
      "@libs/*": ["./src/lib/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
